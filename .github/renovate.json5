{
  assignees: [
    "@kitos9112",
  ],
  commitBodyTable: true,
  dependencyDashboardTitle: "Renovate Dashboard ðŸ¤–",
  "extends": [
    ":enableRenovate",
    "config:recommended",
    ":disableRateLimiting",
    ":dependencyDashboard",
    ":semanticCommits",
    ":separatePatchReleases",
    "docker:enableMajor",
    ":enablePreCommit",
    "github>kitos9112/k8s-home//.github/renovate/autoMerge.json",
    "github>kitos9112/k8s-home//.github/renovate/labels.json",
    "github>kitos9112/k8s-home//.github/renovate/groups.json5",
    "github>kitos9112/k8s-home//.github/renovate/semanticCommits.json",
    "github>kitos9112/k8s-home//.github/renovate/regexManagers.json5",
  ],
  flux: {
    fileMatch: [
      "cluster/.+\\.ya?ml$",
    ],
  },
  gitAuthor: "henry-pa-bot <***+henry-bot[bot]@users.noreply.github.com>",
  "helm-values": {
    fileMatch: [
      "cluster/.+\\.ya?ml$",
    ],
  },
  kubernetes: {
    fileMatch: [
      "cluster/.+\\.yaml$",
    ],
  },
  packageRules: [
    {
      ignoreDeprecated: true,
      matchDatasources: [
        "helm",
      ],
    },
    {
      commitMessageExtra: "to {{newVersion}}",
      matchDatasources: [
        "docker",
      ],
    },
    {
      matchDatasources: [
        "docker",
      ],
      matchPackageNames: [
        "blakeblackshear/frigate",
      ],
      versioning: "regex:^(?<major>\\d+)\\.(?<minor>\\d+)\\.(?<patch>\\d+)-(?<compatibility>.*)$",
    },
    {
      matchDatasources: [
        "docker",
      ],
      matchPackageNames: [
        "quay.io/minio/minio",
      ],
      versioning: "regex:^RELEASE\\.(?<major>\\d+)-(?<minor>\\d+)-(?<patch>\\d+)T.*Z(-(?<compatibility>.*))?$",
    },
    {
      matchDatasources: [
        "github-releases",
      ],
      matchPackageNames: [
        "vmware-tanzu/helm-charts",
        "prometheus-community/helm-charts",
        "1Password/connect-helm-charts",
      ],
      versioning: "regex:^(?<compatibility>.*)-(?<major>\\d+)\\.(?<minor>\\d+)\\.(?<patch>\\d+)$",
    },
    {
      matchDatasources: [
        "docker",
        "github-releases",
      ],
      matchPackageNames: [
        "ghcr.io/onedr0p/plex",
        "ghcr.io/onedr0p/qbittorrent",
        "Hypfer/Valetudo",
      ],
      versioning: "loose",
    },
    {
      additionalBranchPrefix: "",
      groupName: "external-snapshotter-suite",
      matchDatasources: [
        "github-releases",
        "docker",
      ],
      matchPackagePatterns: [
        "external-snapshotter",
        "snapshot-controller",
      ],
      separateMinorPatch: true,
    },
    {
      additionalBranchPrefix: "",
      groupName: "system-upgrade-controller-suite",
      matchDatasources: [
        "github-tags",
        "docker",
      ],
      matchPackagePatterns: [
        "rancher/system-upgrade-controller",
      ],
      separateMinorPatch: true,
    },
    {
      additionalBranchPrefix: "",
      groupName: "vector-suite",
      matchDatasources: [
        "docker",
        "github-releases",
        "helm",
      ],
      matchPackagePatterns: [
        "vector",
      ],
      separateMinorPatch: true,
    },
    {
      description: "Disable kubernetes-api",
      enabled: false,
      matchDatasources: [
        "kubernetes-api",
      ],
      matchManagers: [
        "kubernetes",
      ],
    },
  ],
  rebaseWhen: "conflicted",
  suppressNotifications: [
    "prIgnoreNotification",
  ],
  timezone: "Europe/London",
}
